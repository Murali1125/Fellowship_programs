    "import math\n",
    "t = float(input(\"temperature= \"))\n",
    "v = float(input(\"wind speed= \"))\n",
    "\n",
    "if (v in range(3,120)) and t<=50:\n",
    "    wind_chill = 35.74+(0.6215*t) +((0.4275*t)-35.75)*math.pow(v,0.16)\n",
    "    print(\"wind_chill = \", wind_chill)\n",
    "else:\n",
    "    print(\"values are not in range\")"
